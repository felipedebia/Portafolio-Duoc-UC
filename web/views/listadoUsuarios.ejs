<%- include('partials/nav.ejs') %> 

      <div id="wrapper">
         <div class="container-fluid">
            <p class="lead">
            <h3>Listado de usuarios registradosxD</h3>
            </p>
            <div class="row">
               <div class="col">
                  <div class="card">
                     <div class="card-body">
                        <table class="table">
                           <thead>
                              <tr>
                                 <th scope="col">#</th>
                                 <th scope="col">Nombre</th>
                                 <th scope="col">Apellido</th>
                                 <th scope="col">Correo electr√≥nico</th>
                                 <th scope="col">Opciones</th>
                              </tr>
                           </thead>
                           <tbody>
                              <% for(var i=0; i < Usuarios.length; i++) { %>
                              <tr>
                                 <th scope="row"><%= i %></th>
                                 <td><%= Usuarios[i].nombre %></td>
                                 <td><%= Usuarios[i].apellido %></td>
                                 <td><%= Usuarios[i].correo %></td>
                                 <td><a class="card-link" href="/modificarUsuario/<%= Usuarios[i].id_usuario %>">Modificar</a><a class="card-link" href="/eliminarUsuario/<%= Usuarios[i].id_usuario %>">Eliminar</a></td>
                              </tr>
                            <% } %>
                           </tbody>
                        </table>
                     </div>
                  </div>
               </div>
            </div>
         </div>
      </div>

</body>
</html>