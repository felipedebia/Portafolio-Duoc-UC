                    <li class="nav-item <% if (navActive == 'MisPedidos'){ %>active<% } %>">
                        <a class="nav-link" href="../mispedidos">
                            <i class="material-icons">loyalty</i>
                            <p>Mis Pedidos</p>
                        </a>
                    </li>
                    <li class="nav-item <% if (navActive == 'MisCompras'){ %>active<% } %>">
                        <a class="nav-link" href="../miscompras">
                            <i class="material-icons">shopping_bag</i>
                            <p>Mis Compras</p>
                        </a>
                    </li>
                  <% } %>
